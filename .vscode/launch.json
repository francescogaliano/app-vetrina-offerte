    "version": "0.2.0",
    "configurations": [

    
        {
            "name": "vetrina_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "vetrina_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "vetrina_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "vetrina_app (Flutter Web con CORS disabilitato)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--web-browser-flag=--disable-web-security",
                "--web-browser-flag=--user-data-dir=/tmp/tempChromeDev"
            ]
        }
    ]
}